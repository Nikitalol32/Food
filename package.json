{
	"name": "food",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"dev": "vite",
		"build": "run-p type-check build-only",
		"server": "ts-node --project tsconfig.server.json ./src/server/app.ts",
		"preview": "vite preview",
		"build-only": "vite build",
		"cy:open": "cypress open",
		"cy:run": "cypress run",
		"type-check": "vue-tsc --noEmit",
		"run:servers": "concurrently \"npm run dev\" \"npm run server\"",
		"lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
	},
	"dependencies": {
		"concurrently": "^8.0.1",
		"wait-on": "7.0.1",
		"cors": "^2.8.5",
		"upath": "^2.0.1",
		"vue": "^3.2.47",
		"vue-router": "^4.0.13"
	},
	"devDependencies": {
		"@rushstack/eslint-patch": "^1.2.0",
		"@types/cors": "^2.8.13",
		"@types/cypress": "1.1.3",
		"@types/express": "4.17.17",
		"@types/node": "^18.14.2",
		"@vitejs/plugin-vue": "^4.0.0",
		"@vue/eslint-config-typescript": "^11.0.2",
		"@vue/tsconfig": "^0.1.3",
		"cypress": "12.9.0",
		"eslint": "^8.34.0",
		"eslint-plugin-vue": "^9.9.0",
		"express": "4.18.2",
		"npm-run-all": "^4.1.5",
		"ts-node": "10.9.1",
		"tsconfig-paths": "4.2.0",
		"typescript": "~4.8.4",
		"vite": "^4.1.4",
		"vue-tsc": "^1.2.0"
	}
}
